.detailedListOk {
  display: flow-root;
  padding: 0 30rpx;
  overflow-x: hidden;
  min-height: 100vh;
  background-image: -webkit-linear-gradient(top, #faebd4, #f6a1d8);
  background-image: linear-gradient(to bottom, #faebd4, #f6a1d8);
}
.detailedList-number {
  padding: 20rpx 0;
}
.detailedList-list {
  width: 330rpx;
  height: 330rpx;
  float: left;
  margin: 15rpx 0;
  border-radius: 30rpx;
  margin-left: 15rpx;
  background: black;
}
.detailedList-list image {
  width: 334rpx;
  height: 330rpx;
  border-radius: 30rpx;
  opacity: .5;
}
.detailedList-list_text {
  width: 250rpx;
  height: 60rpx;
  overflow-x: hidden;
  color: #FFFFFF;
  font-size: 40rpx;
  position: relative;
  top: -200rpx;
  left: 12%;
}

