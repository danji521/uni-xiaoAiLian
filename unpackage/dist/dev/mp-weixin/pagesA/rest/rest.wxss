.sleep {
  text-align: center;
  width: 100%;
  height: 100vh;
  background-image: url(data:image/png;base64,);
  background-size: 100% 100%;
}
.getup {
  background-image: url(data:image/png;base64,);
}
.sleep-time {
  position: relative;
  top: 220rpx;
  font-size: 140rpx;
}
.sleep-date {
  position: relative;
  top: 220rpx;
}
.sleep-lovers {
  position: relative;
  top: 336rpx;
}
.sleep-lovers image {
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
}
.sleep-lovers .active {
  width: 50rpx;
  height: 50rpx;
  border-radius: 50%;
}
