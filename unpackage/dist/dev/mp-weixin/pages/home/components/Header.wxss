.header {
  background-image: -webkit-linear-gradient(left, #FEDADE, #FF788C);
  background-image: linear-gradient(to right, #FEDADE, #FF788C);
  z-index: 1;
  height: 342rpx;
}
.header .header-up {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  padding-top: 60rpx;
  color: #FFFFFF;
  font-size: 46rpx;
}
.header .header-up image {
  width: 88rpx;
  height: 88rpx;
  border-radius: 50%;
  border: 1rpx solid #FFFFFF;
}
.header .header-up view {
  margin: 0 36rpx;
  margin-top: -30rpx;
}
.header .header-bt {
  position: relative;
  top: -20rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  margin: 0 auto;
  height: 80rpx;
  width: 148rpx;
  color: #FF868C;
  font-size: 36rpx;
  font-weight: 600;
  background-image: url(data:image/png;base64,);
  background-size: 100% 100%;
}
.header .header-bt view {
  font-size: 26rpx;
  margin-top: 10rpx;
}

